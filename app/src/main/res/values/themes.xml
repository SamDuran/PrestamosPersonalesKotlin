<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Theme.PrestamosPersonales" parent="android:Theme.Material.NoActionBar">
        <item name="android:statusBarColor">@color/StatusBar</item>
        <item name="backgroundColor">#00FFFFFF</item>
        <item name="android:windowBackground">@null</item>
    </style>

    <style name="Theme.PrestamosPersonales.Fullscreen" parent="Theme.PrestamosPersonales">
        <item name="android:actionBarStyle">
            @style/Widget.Theme.PrestamosPersonales.ActionBar.Fullscreen
        </item>
        <item name="android:windowActionBarOverlay">true</item>
        <item name="android:windowBackground">@null</item>
    </style>

    <style name="ThemeOverlay.PrestamosPersonales.FullscreenContainer" parent="">
        <item name="fullscreenBackgroundColor">@color/light_blue_600</item>
        <item name="fullscreenTextColor">@color/light_blue_A200</item>
    </style>


</resources>